<UserControl x:Class="inStaDemo.UserControls.VideoPostUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:inStaDemo.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel>
        <ContentControl MouseDoubleClick="ContentControl_MouseDoubleClick">
            <MediaElement x:Name="VideoOfPost" />
        </ContentControl>
        <local:PostOperationsUC x:Name="PostOp" Height="40"/>
        <local:PostLikeByUC/>

    </StackPanel>
</UserControl>
